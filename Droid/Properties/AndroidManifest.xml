<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android" android:versionCode="1" android:versionName="1.0" package="bigslickchat.android">
	<uses-sdk android:minSdkVersion="15" android:targetSdkVersion="23" />
	<application android:label="BigSlickChat">
		<receiver android:name="com.onesignal.GcmBroadcastReceiver"
	            android:permission="com.google.android.c2dm.permission.SEND" >
		    <intent-filter>
		      <action android:name="com.google.android.c2dm.intent.RECEIVE" />
		      <category android:name="bigslickchat.android" />
		    </intent-filter>
  		</receiver>
	</application>
	<permission android:name="bigslickchat.android.permission.C2D_MESSAGE"
            android:protectionLevel="signature" />
	<uses-permission android:name="bigslickchat.android.permission.C2D_MESSAGE" />
</manifest>